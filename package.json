{
  "name": "viewar-react-boilerplate",
  "version": "1.3.1",
  "description": "React boilerplate app for ViewAR SDK",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build && rimraf bundle",
    "deploy": "viewar-cli deploy",
    "build": "npm run clean && webpack -p --mode production --env production",
    "start": "npm run clean && webpack-dev-server --open --mode development --env development_core",
    "start:mock": "npm run clean && webpack-dev-server --open --mode development --env development_mock",
    "test": "npm run lint && npm run build",
    "lint": "npm run clean && eslint . --format pretty --fix && prettier ./src/**/*.{css,scss,md,json} --write --loglevel silent"
  },
  "author": "Karl Hofer <kh@viewar.com>",
  "license": "ISC",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "repository": "https://github.com/viewar/viewar-boilerplate-react",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/commit-analyzer": "^6.3.0",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/github": "^5.4.3",
    "@semantic-release/npm": "^5.1.15",
    "@semantic-release/release-notes-generator": "^7.3.0",
    "@viewar/webpack": "git+https://github.com/DoubleU23/viewar-webpack.git",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "css-loader": "^3.2.0",
    "copy-webpack-plugin": "^5.0.4",
    "eslint": "^6.4.0",
    "eslint-config-viewar": "git+https://github.com/DoubleU23/eslint-config-viewar.git",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "postcss": "^7.0.7",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.5.0",
    "prettier": "^1.15.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.3.12",
    "rimraf": "^3.0.0",
    "sass-loader": "^7.1.0",
    "semantic-release-cli": "^5.2.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.2.0",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.1.5"
  },
  "dependencies": {
    "fastclick": "^1.0.6",
    "normalize.css": "^8.0.1",
    "viewar-api": "^0.56.1",
    "viewar-core": "^11.34.2"
  }
}
